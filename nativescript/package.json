  "scripts": {
    "preinstall": "mkdirp app",
    "clean": "rimraf platforms node_modules lib hooks app && mkdirp app",
    "tslint": "tslint -p tsconfig.json",
    "debug-android": "tns debug android",
    "debug-ios": "tns debug ios",
    "start-android": "tns run android",
    "start-ios": "tns run ios",
    "build-android": "tns build android",
    "build-ios": "tns build ios",
    "generate-android-snapshot": "generate-android-snapshot --targetArchs arm,arm64,ia32 --install",
    "e2e": "tsc -p e2e && mocha --opts ./e2e/config/mocha.opts",
    "compile-tests": "tsc -p e2e --watch"
  },
